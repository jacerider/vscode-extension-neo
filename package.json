{
  "name": "vscode-extension-neo",
  "displayName": "Drupal Neo Extension Pack",
  "description": "Visual Studio Code extensions for developing Drupal based Neo projects.",
  "version": "1.0.0",
  "publisher": "jacerider",
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  },
  "qna": "https://github.com/jacerider/vscode-extension-neo/issues",
  "bugs": {
    "url": "https://github.com/jacerider/vscode-extension-neo/issues"
  },
  "repository": {
    "url": "https://github.com/jacerider/vscode-extension-neo"
  },
  "icon": "icon.png",
  "categories": [
    "Extension Packs"
  ],
  "contributes": {
    "configurationDefaults": {
      "[css]": {
        "editor.defaultFormatter": "stylelint.vscode-stylelint",
        "editor.codeActionsOnSave": {
          "source.fixAll.stylelint": "explicit"
        }
      },
      "[scss]": {
        "editor.defaultFormatter": "stylelint.vscode-stylelint",
        "editor.codeActionsOnSave": {
          "source.fixAll.stylelint": "explicit"
        }
      },
      "stylelint.validate": [
        "css",
        "scss"
      ],
      "headwind.runOnSave": true,
      "typescript.tsdk": "node_modules/typescript/lib"
    }
  },
  "extensionPack": [
    "stylelint.vscode-stylelint",
    "bradlc.vscode-tailwindcss",
    "heybourn.headwind"
  ]
}
